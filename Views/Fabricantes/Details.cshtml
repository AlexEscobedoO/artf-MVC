@model artf_MVC.Models.Fabricante

@{
    ViewData["Title"] = "Details";
}

<div>
    <h4>Detalles del fabricante</h4>

    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Rsfab)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Rsfab)
        </dd>
    </dl>
    <dl class = "row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Marfab)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Marfab)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" class="btn btn-warning" asp-route-id="@Model?.Idfab">Editar</a> 
    <a asp-action="Index" class="btn btn-info">Volver a la lista</a>
</div>
