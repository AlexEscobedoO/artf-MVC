@model artf_MVC.Models.Insrf

@{
    ViewData["Title"] = "Create";
}

<h4>Crear Inscripción</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Idempins" class="control-label"></label>
                <select asp-for="Idempins" class ="form-control" asp-items="ViewBag.Idempins"></select>
            </div>
            <div class="form-group">
                <label asp-for="Idsolins" class="control-label"></label>
                <select asp-for="Idsolins" class ="form-control" asp-items="ViewBag.Idsolins"></select>
            </div>
           @*  <div class="form-group">
                <label asp-for="Iduserins" class="control-label"></label>
                <select asp-for="Iduserins" class ="form-control" asp-items="ViewBag.Iduserins"></select>
            </div> *@
            <div class="form-group">
                <label asp-for="Numacuofins" class="control-label"></label>
                <input asp-for="Numacuofins" class="form-control" />
                <span asp-validation-for="Numacuofins" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Obsins" class="control-label"></label>
                <textarea asp-for="Obsins" class="form-control" rows="5" maxlength="200"></textarea>
                <span asp-validation-for="Obsins" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Fecapins" class="control-label"></label>
                <input asp-for="Fecapins" class="form-control" />
                <span asp-validation-for="Fecapins" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Docins" class="control-label"></label>
                <input type="file" asp-for="Docins" class="form-control" />
                <span asp-validation-for="Docins" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-success" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" class="btn btn-info">Volver a la lista</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
