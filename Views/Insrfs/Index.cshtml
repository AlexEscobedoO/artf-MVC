@model IEnumerable<artf_MVC.Models.Insrf>

@{
    ViewData["Title"] = "Index";
}

<h1>Inscripción de solicitud</h1>

<p>
    <a asp-action="Create" class="btn btn-success">Registro nuevo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Idins)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Numacuofins)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Obsins)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecapins)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Docins)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdempinsNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdsolinsNavigation)
            </th>
@*             <th>
                @Html.DisplayNameFor(model => model.IduserinsNavigation)
            </th> *@
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                    @Html.DisplayFor(modelItem => item.Idins)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Numacuofins)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Obsins)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecapins)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Docins)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdempinsNavigation.Rsempre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdsolinsNavigation.Idsol)
            </td>
            @* <td>
                @Html.DisplayFor(modelItem => item.IduserinsNavigation.Iduser)
            </td> *@
            <td>
                <a asp-action="Edit" class="btn btn-default btn-sm" asp-route-id="@item.Idins">Editar</a>
                <a asp-action="Details" class="btn btn-danger btn-sm" asp-route-id="@item.Idins">Detalles</a>
                <a asp-action="Delete" class="btn btn-primary btn-sm" asp-route-id="@item.Idins">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
